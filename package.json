{
  "name": "gym-diary",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@gym-diary/exercise": "file:./packages/exercise",
    "@gym-diary/strength-score": "file:./packages/strength-score"
  },
  "scripts": {
    "dev:api": "npm --workspace apps/api run start:dev",
    "dev:mobile": "npm --workspace apps/mobile run start",
    "dev:web": "npm --workspace apps/web run dev"
  }
}
